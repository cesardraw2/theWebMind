<root>
	<none>nada,nenhum,zero,0,nenhuma</none>
	<one>um,uma,1,algum,alguma</one>
	<many>uns,alguns,umas,algumas,muito,muita,muitos,vários,muitas,várias,diversos,diversas,n,m,x</many>
	<or>ou,até,/</or>
</root>